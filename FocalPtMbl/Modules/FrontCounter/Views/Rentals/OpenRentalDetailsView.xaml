    <dxn:TabPageItem >
        <dxn:TabPageItem.Content>
            <ContentPage Title="Availability"   BackgroundColor="{DynamicResource BackgroundThemeColor}">
                <StackLayout>
                    <Grid RowSpacing="0" BackgroundColor="White" HorizontalOptions="FillAndExpand" VerticalOptions="Start">
                        <!--<Grid.RowDefinitions>
					<RowDefinition   />
				</Grid.RowDefinitions>-->
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="35*" />
                            <ColumnDefinition Width="65*" />
                        </Grid.ColumnDefinitions>
                        <dxe:DateEdit Date="{Binding StartDate}" DisplayFormat="d" DateChanged="DateEdit_DateChanged" Grid.Row="0" Grid.Column="0" LabelText="Start Date" ReturnType="Default"></dxe:DateEdit>
                        <dxe:TimeEdit Time="{Binding StartTime}" TimeChanged="TimeEdit_TimeChanged" Grid.Row="0" Grid.Column="1" LabelText="Start Time" ReturnType="Default" ></dxe:TimeEdit>

                        <dxe:DateEdit Date="{Binding EndDate}" DisplayFormat="d" DateChanged="DateEdit_DateChanged_1" Grid.Row="1" Grid.Column="0"  LabelText="End Date" ReturnType="Default"></dxe:DateEdit>
                        <dxe:TimeEdit Time="{Binding EndTime}" TimeChanged="TimeEdit_TimeChanged_1" Grid.Row="1" Grid.Column="1"  LabelText="End Time" ReturnType="Default" ></dxe:TimeEdit>
                        <dxe:SimpleButton Grid.Row="2" Grid.ColumnSpan="2" TextColor="White" BackgroundColor="#2088ff" Text="Search" Clicked="SimpleButton_Clicked" />

                        <Label Text="  Availability  " Grid.Row="3" Grid.Column="0"  TextColor="Black" XAlign="Start"     />
                        <Label Text="{Binding CurrentRental.RentalDscr}" Grid.Row="3" Grid.Column="1"  TextColor="Black"   BackgroundColor="White" />
                        <Label Text="Store:  " Grid.Row="4" Grid.Column="0" TextColor="Black" XAlign="End"     />
                        <Label Text="{Binding CurrentRental.RentalBelongsTo}" Grid.Row="4" Grid.Column="1" TextColor="Black"   />
                        <Label Text="Equipment ID:  " Grid.Row="5" Grid.Column="0" TextColor="Black" XAlign="End"     />
                        <Label Text="{Binding CurrentRental.RentalEquipID}" Grid.Row="5" Grid.Column="1" TextColor="Black"   />
                        <Label Text="Manufacturer:  " Grid.Row="6" Grid.Column="0" TextColor="Black" XAlign="End"     />
                        <Label Text="{Binding CurrentRental.RentalEquipID}" Grid.Row="6" Grid.Column="1" TextColor="Black"   />
                    </Grid>
                </StackLayout>
            </ContentPage>
        </dxn:TabPageItem.Content>
    </dxn:TabPageItem>
